{
  "name": "coverage-diff-back",
  "version": "0.1.0",
  "description": "Send the coverage difference back to the github on each pull requests",
  "bin": {
    "coverage-diff-back": "dist/cli.js"
  },
  "scripts": {
    "commit": "git-cz",
    "prepare": "babel src --out-dir dist --verbose --extensions '.ts'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "repository": "Leko/coverage-diff-back",
  "keywords": [
    "ci",
    "test",
    "coverage",
    "report",
    "github",
    "pull request"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leko/coverage-diff-back/issues"
  },
  "homepage": "https://github.com/Leko/coverage-diff-back#readme",
  "dependencies": {
    "@octokit/graphql": "^2.0.2",
    "@octokit/rest": "^16.1.0",
    "deindent": "^0.1.0",
    "env-ci": "^3.1.2",
    "glob": "^7.1.3",
    "node-fetch": "^2.3.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/glob": "^7.1.1",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/node-fetch": "^2.1.4",
    "@types/yargs": "^12.0.1",
    "commitizen": "^3.0.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.2.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.3",
    "typescript": "^3.2.1"
  }
}
